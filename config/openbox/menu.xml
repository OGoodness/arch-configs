<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<item label="Terminal">
			<action name="Execute">
				<execute>kitty</execute>
			</action>
		</item>
		<item label="Browser">
			<action name="Execute">
				<execute>firefox</execute>
			</action>
		</item>
		<separator/>
		<menu id="root-menu-87617" label="Terminal Apps">
			<item label="Music">
				<action name="Execute">
					<execute>kitty -e ncmcpp</execute>
				</action>
			</item>
			<item label="Files">
				<action name="Execute">
					<execute>kitty -e ranger</execute>
				</action>
			</item>
		</menu>
		<menu id="root-menu-35823" label="GUI Apps">
			<item label="Music">
				<action name="Execute">
					<execute>cantata</execute>
				</action>
			</item>
			<item label="Files">
				<action name="Execute">
					<execute>thunar</execute>
				</action>
			</item>
			<item label="Code">
				<action name="Execute">
					<execute>codium</execute>
				</action>
			</item>
		</menu>
		<menu id="root-menu-301512" label="Settings">
			<item label="OB reconfigure">
				<action name="Reconfigure"/>
			</item>
			<item label="OB Conf">
				<action name="Execute">
					<execute>obconf</execute>
				</action>
			</item>
			<item label="OB Menu">
				<action name="Execute">
					<execute>obmenu</execute>
				</action>
			</item>
			<item label="GTK settings">
				<action name="Execute">
					<execute>lxappearance</execute>
				</action>
			</item>
			<item label="Shortcuts">
				<action name="Execute">
					<execute>xbindkeys_config</execute>
				</action>
			</item>
			<menu id="root-menu-301512-302950" label="DotFiles">
				<item label=".config">
					<action name="Execute">
						<execute>codium ~/.config</execute>
					</action>
				</item>
				<item label="autostart">
					<action name="Execute">
						<execute>codium ~/.config/openbox/autostart</execute>
					</action>
				</item>
				<item label="polybar">
					<action name="Execute">
						<execute>codium ~/.config/polybar/config</execute>
					</action>
				</item>
				<item label="xresources">
					<action name="Execute">
						<execute>codium ~/.Xresources</execute>
					</action>
				</item>
				<item label="vimrc">
					<action name="Execute">
						<execute>codium ~/.config/nvim/init.vim</execute>
					</action>
				</item>
				<item label="obrc">
					<action name="Execute">
						<execute>codium ~/.config/openbox/rc.xml</execute>
					</action>
				</item>
			</menu>
			<menu id="root-menu-301512-761739" label="Redshift">
				<item label="Redshift  start">
					<action name="Execute">
						<execute>redshift</execute>
					</action>
				</item>
				<item label="Redshift  stop">
					<action name="Execute">
						<execute>pkill redshift</execute>
					</action>
				</item>
			</menu>
		</menu>
		<!-- This requires the presence of the 'menu' package to work -->
		<menu id="root-menu-32250" label="System">
			<item label="Suspend">
				<action name="Execute">
					<execute>systemctl suspend</execute>
				</action>
			</item>
			<item label="Poweroff">
				<action name="Execute">
					<execute>systemctl poweroff</execute>
				</action>
			</item>
			<item label="Reboot">
				<action name="Execute">
					<execute>systemctl reboot</execute>
				</action>
			</item>
		</menu>
	</menu>
</openbox_menu>
